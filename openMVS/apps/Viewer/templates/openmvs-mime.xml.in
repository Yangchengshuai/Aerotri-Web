<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-openmvs-mvs">
    <comment>OpenMVS scene file</comment>
    <glob pattern="*.mvs"/>
    <icon> @ICON_NAME@ </icon>
  </mime-type>
  <mime-type type="application/x-openmvs-dmap">
    <comment>OpenMVS depth map file</comment>
    <glob pattern="*.dmap"/>
    <icon> @ICON_NAME@ </icon>
  </mime-type>
</mime-info>
